# Porta do servidor HTTP que hospeda a API e o painel web
PORT=3000

# Identificador único para a sessão do WhatsApp (uma por computador/setor)
WHATSAPP_SESSION=default

# Nome do analista logado nesta máquina
ANALYST_NAME=Seu Nome

# ID da planilha "Atendimentos CCA" no Google Sheets
GOOGLE_SHEET_ID=

# Credenciais de serviço do Google (JSON serializado)
GOOGLE_CLIENT_EMAIL=
GOOGLE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----\n"

# (Opcional) ID do projeto Google Cloud caso use autenticação avançada
GOOGLE_PROJECT_ID=
